  },
  "peerDependencies": {
    "@fxhash/client-sdk": "workspace:*",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@fxhash/tsconfig": "workspace:*",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.3.0",
    "tsup": "8.0.1",
    "typescript": "5.3.3"
  }
