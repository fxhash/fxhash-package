{
  "name": "@fxhash/client-plugnplay-react",
  "description": "A react wrapper on @fxhash/client-plugnplay, implementing React providers & hooks for ease-of-use.",
  "private": true,
  "version": "0.0.1",
  "author": "fxhash",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fxhash/fxhash-package"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
