    "react-colorful": "5.6.1",
    "sha1": "1.1.1",
    "zod": "3.21.4"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@fxhash/eslint-config": "workspace:*",
    "@fxhash/prettier-config": "workspace:*",
    "@types/lodash.debounce": "4.0.7",
    "@types/node-sass": "4.11.3",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@types/sha1": "1.1.3",
