  "repository": "fxhash/fxhash-package",
  "type": "module",
  "types": "./dist/fxhash.d.ts",
  "scripts": {
    "build": "tsup --config tsup.config.version.ts",
    "build-sdk": "pnpm run build && tsup --config tsup.config.sdk.ts",
    "dev-sdk": "tsup --watch"
  }
}