{
  "name": "@fxhash/project-sdk",
  "version": "0.0.14",
  "author": "fxhash",
  "dependencies": {
    "@fxhash/params": "workspace:*",
    "@fxhash/utils": "workspace:*"
  },
  "devDependencies": {
    "@esbuild-plugins/node-modules-polyfill": "0.2.2",
    "@types/node": "18.7.13",
    "tsc-alias": "1.8.5",
    "tslib": "2.6.2",
    "tsup": "8.4.0",
    "typescript": "5.8.2",
    "@fxhash/eslint-config": "workspace:*",
    "@fxhash/tsconfig": "workspace:*"
  },
  "exports": {
    ".": {
