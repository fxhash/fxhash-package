    },
    "./version": {
      "types": "./dist/version.d.ts",
      "import": "./dist/version.js"
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "main": "./dist/fxhash.min.js",
  "module": "./dist/fxhash.min.js",
