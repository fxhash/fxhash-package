    "@fxhash/eslint-config": "workspace:*",
    "@fxhash/tsconfig": "workspace:*",
    "@types/node": "18.7.13",
    "tsc-alias": "1.8.5",
    "tslib": "2.6.0",
    "tsup": "8.0.1",
    "typescript": "5.3.3"
