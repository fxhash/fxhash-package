    "autonomy-irl-js": "1.0.0"
  },
  "peerDependencies": {
    "@taquito/beacon-wallet": "^17.3.1",
    "@taquito/taquito": "^17.3.1",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@taquito/beacon-wallet": "17.3.1",
    "@taquito/taquito": "17.3.1",
    "@types/react": "18.2.15",
    "typescript": "5.0.4",
    "tsup": "6.6.0"
