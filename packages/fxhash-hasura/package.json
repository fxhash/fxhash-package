{
  "name": "@fxhash/hasura",
  "version": "1.0.0",
  "description": "Hasura GQL API client for FXHASH",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    "./*": "./dist/*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "gen-client": "genql --endpoint http://localhost:8888/v1/graphql --output src -H 'x-hasura-admin-secret: changeme'",
    "test": "jest"
  },
  "author": "fxhash",
  "devDependencies": {
    "@genql/cli": "2.10.0",
    "@jest/globals": "29.6.4",
    "graphql-ws": "5.14.0",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "tslib": "2.6.2",
    "tsup": "6.6.0",
    "ws": "8.14.2"
  }
}
