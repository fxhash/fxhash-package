{
  "name": "@fxhash/fxhash-snippet",
  "version": "0.0.7",
  "author": "fxhash",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fxhash/fxhash-package"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "node-html-parser": "6.1.5"
  },
  "devDependencies": {
    "@esbuild-plugins/node-modules-polyfill": "0.2.2",
    "@types/node": "18.7.13",
    "tsc-alias": "1.8.5",
    "tsup": "6.6.0",
    "typescript": "4.9.5",
    "@fxhash/prettier-config": "1.0.2",
    "@fxhash/eslint-config": "1.0.2"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  }
}