{
  "name": "@fxhash/client-basic",
  "description": "A basic client wrapping @fxhash/client-sdk by providing a medium-level API where all the @fxhash/client-sdk modules are properly configured together. Should be used for medium/advanced use cases.",
  "version": "0.0.1",
  "author": "fxhash",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fxhash/fxhash-package"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:local": "FXHASH_ENV=local vitest",
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@fxhash/client-sdk": "workspace:*",
    "@fxhash/shared": "workspace:*",
    "@fxhash/utils": "workspace:*"
  },
  "devDependencies": {
    "@fxhash/eslint-config": "workspace:*",
    "@fxhash/tsconfig": "workspace:*",
    "@types/eslint": "8.44.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.45.0",
    "tsup": "8.0.1",
    "typescript": "5.3.3",
    "vitest": "0.34.6"
  }
}
