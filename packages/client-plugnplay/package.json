{
  "name": "@fxhash/client-plugnplay",
  "description": "A plug'n'play client interface to build applications with fxhash. This packages handles everything related to wallets & authentication and provides very high level functions for building applications on top of fxhash.",
  "version": "0.0.1",
  "author": "fxhash",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fxhash/fxhash-package"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:local": "FXHASH_ENV=local vitest",
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@airgap/beacon-sdk": "4.2.2",
    "@fxhash/client-sdk": "workspace:*",
    "@fxhash/config": "workspace:*",
    "@fxhash/eth": "workspace:*",
    "@fxhash/shared": "workspace:*",
    "@fxhash/tez": "workspace:*",
    "@fxhash/utils": "workspace:*",
    "@fxhash/utils-browser": "workspace:*",
    "@tanstack/react-query": "5.28.8",
    "@taquito/beacon-wallet": "19.2.1",
    "@taquito/taquito": "17.3.1",
    "@wagmi/core": "2.11.7",
    "connectkit": "1.7.3",
    "viem": "2.13.10",
    "wagmi": "2.10.10"
  },
  "devDependencies": {
    "@fxhash/eslint-config": "workspace:*",
    "@fxhash/tsconfig": "workspace:*",
    "@types/eslint": "8.44.0",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.45.0",
    "tsup": "8.0.1",
    "typescript": "5.3.3",
    "vitest": "0.34.6"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "^18.2.0"
  }
}
