{
  "extends": "@fxhash/tsconfig/base",
  "include": ["src/**/*", "test"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "dist",
    "types": ["vitest/globals"],
    "paths": {
      "@/*": ["./src/*"]
    },
    "strict": true,
    "useUnknownInCatchVariables": false,
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "maxNodeModuleJsDepth": 10
  }
}
