{
  "extends": "@fxhash/tsconfig/react",
  "include": ["src/**/*", "test/**/*", "./declaration.d.ts"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "rootDir": "src",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["vitest/globals"],
    "outDir": "dist",
    "stripInternal": true
  }
}
