{
  "extends": "@fxhash/tsconfig/react",
  "include": ["src/**/*", "./declaration.d.ts"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "paths": {
      "@/*": ["./src/*"]
    },
    "outDir": "dist",
    "stripInternal": true,
    "allowJs": false,
    "declaration": true,
    "isolatedDeclarations": true
  }
}
