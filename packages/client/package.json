{
  "name": "@fxhash/client",
  "description": "A general-purpose nodejs/browser compatible utility package related to fxhash applications.",
  "version": "0.0.1",
  "author": "fxhash",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fxhash/fxhash-package"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:local": "FXHASH_ENV=local vitest",
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@airgap/beacon-sdk": "4.2.2-beta.3",
    "@fxhash/tez": "workspace:*",
    "@fxhash/eth": "workspace:*",
    "@fxhash/gql": "workspace:*",
    "@fxhash/gql-client": "workspace:*",
    "@fxhash/shared": "workspace:*",
    "@fxhash/utils": "0.0.2",
    "jwt-decode": "4.0.0"
  },
  "devDependencies": {
    "@types/eslint": "8.44.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.45.0",
    "@fxhash/config": "workspace:*",
    "@fxhash/eslint-config": "workspace:*",
    "@fxhash/eth": "workspace:*",
    "@fxhash/tez": "workspace:*",
    "@fxhash/tsconfig": "workspace:*",
    "@taquito/beacon-wallet": "19.2.1-beta.1",
    "@taquito/signer": "17.3.1",
    "@taquito/taquito": "17.3.1",
    "tsup": "8.0.1",
    "typescript": "5.3.3",
    "vitest": "0.34.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "viem": "2.9.2",
    "wagmi": "2.12.5"
  },
  "peerDependencies": {
    "@fxhash/config": "workspace:^",
    "@fxhash/eth": "workspace:*",
    "@fxhash/tez": "workspace:*",
    "@taquito/beacon-wallet": "19.2.1-beta.1",
    "@taquito/signer": "17.3.1",
    "@taquito/taquito": "17.3.1",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "viem": "2.9.2",
    "wagmi": "2.12.5"
  }
}
