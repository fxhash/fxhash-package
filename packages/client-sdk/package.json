{
  "name": "@fxhash/client-sdk",
  "description": "Low-level building blocks to compose a fxhash client application. Should only be used directly for advanced use cases.",
  "version": "0.0.1",
  "author": "fxhash",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fxhash/fxhash-package"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:local": "FXHASH_ENV=local vitest",
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@airgap/beacon-sdk": "4.2.2-beta.3",
    "@fxhash/eth": "workspace:*",
    "@fxhash/gql": "workspace:*",
    "@fxhash/gql-client": "workspace:*",
    "@fxhash/shared": "workspace:*",
    "@fxhash/tez": "workspace:*",
    "@fxhash/utils": "workspace:*",
    "@fxhash/utils-browser": "workspace:*",
    "@taquito/utils": "17.3.1",
    "@wagmi/connectors": "5.0.22",
    "@wagmi/core": "2.11.7",
    "ethers": "6.8.1",
    "jwt-decode": "4.0.0"
  },
  "devDependencies": {
    "@fxhash/config": "workspace:^",
    "@fxhash/eslint-config": "workspace:*",
    "@fxhash/tsconfig": "workspace:*",
    "@types/eslint": "8.44.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.45.0",
    "msw": "2.3.4",
    "tsup": "6.6.0",
    "typescript": "5.3.3",
    "vitest": "0.34.6"
  },
  "peerDependencies": {
    "@fxhash/config": "workspace:^",
    "@fxhash/eth": "workspace:*",
    "@fxhash/tez": "workspace:*",
    "@taquito/beacon-wallet": "19.2.1-beta.1",
    "@taquito/signer": "17.3.1",
    "@taquito/taquito": "17.3.1",
    "viem": "2.13.10",
    "wagmi": "2.10.10"
  }
}
