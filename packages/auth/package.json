{
  "name": "@fxhash/auth",
  "version": "0.0.8",
  "author": "fxhash",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fxhash/fxhash-package"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "cookie": "0.5.0",
    "express": "4.18.2",
    "jsonwebtoken": "9.0.0"
  },
  "devDependencies": {
    "@fxhash/eslint-config": "workspace:*",
    "@fxhash/prettier-config": "workspace:*",
    "@types/cookie": "0.5.1",
    "@types/express": "4.17.17",
    "@types/jsonwebtoken": "9.0.4",
    "@types/node": "18.7.13",
    "tsup": "6.6.0",
    "typescript": "5.0.4"
  }
}
